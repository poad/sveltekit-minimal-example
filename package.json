		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.17.1",
		"svelte-check": "^4.1.3",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.19.1",
		"vite": "^6.0.7",
		"vitest": "^2.1.8"
	},
	"type": "module",
